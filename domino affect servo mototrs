#include<Servo.h>
Servo a;
Servo b;
Servo c;
Servo d;
Servo e;


const int servobo = 2;

const int servopin = 3;

const int servode = 4;

const int servolo = 5;

const int servoko = 6;



void setup(){
 a.attach(servobo);
 b.attach(servopin);
 c.attach(servode);
 d.attach(servolo);
 e.attach(servoko);
}
void loop(){
a.write(90);
delay(300);
b.write(90);
delay(300);
c.write(90);
delay(300);
e.write(90);
delay(300);
d.write(90);
delay(300);
a.write(0);
delay(300);
b.write(0);
delay(300);
c.write(0);
delay(300);
e.write(0);
delay(300);
d.write(0);
delay(300);
}
